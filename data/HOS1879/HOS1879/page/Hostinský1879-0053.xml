                <Coords points="146,1835 300,1828 413,1843 612,1812 611,1788 588,1787 461,1799 432,1788 389,1805 145,1796"/>
                <Baseline points="153,1819 178,1820 203,1821 228,1822 253,1823 278,1823 303,1824 328,1824 353,1824 379,1823 404,1823 429,1822 454,1821 479,1820 504,1818 529,1815 554,1813 579,1810 605,1806"/>
                <TextEquiv>
                    <Unicode>vertretung gefasst.“</Unicode>
                </TextEquiv>
